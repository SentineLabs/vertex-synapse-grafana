          "opts": {
            "flatten": true
          },
          "refId": "A",
          "stormQuery": "$metrics = $lib.import(metrics)\n$result = ([])\nfor $data in $metrics.aggStatsByTime(\n    $timeRange, scope=\"cortex\", window=3600) {\n    if($data.index(0)='data'){\n        $timestamp = $data.index(1).index(0)\n        $dict = $data.index(1).index(1)\n        $result.append(({\"timestamp\":$timestamp, \"data\":$dict}))\n\n    }  \n}\nreturn($result)",
          "useCall": true
        }
      ],
      "title": "Total Edits",
      "transformations": [
        {
          "id": "filterFieldsByName",
          "options": {
            "byVariable": false,
            "include": {
