{
  "name": "vertex-synapse-datasource",
  "version": "1.0.0",
  "description": "Grafana datasource plugin for Vertex Synapse API",
  "scripts": {
    "build": "npx webpack -c webpack.config.js",
    "dev": "webpack -w -c webpack.config.js",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "npm exec playwright test",
    "e2e:update": "npm exec playwright test --update-snapshots",
    "server": "docker-compose up --build"
  },
  "author": "Grafana Community",
  "license": "MIT",
  "devDependencies": {
    "@grafana/data": "^12.1.0",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/runtime": "^12.1.0",
    "@grafana/ui": "^12.1.0",
    "@swc/core": "^1.3.62",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.0",
    "@types/lodash": "latest",
    "@types/node": "^18.15.11",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "swc-loader": "^0.2.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "rxjs": "7.8.1"
  },
  "engines": {
    "node": ">=16"
  }
}
